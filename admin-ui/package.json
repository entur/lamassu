{
  "name": "lamassu-admin-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build --base /admin/ui/",
    "lint": "eslint .",
    "check": "prettier . --check",
    "format": "prettier --write .",
    "prepare": "husky",
    "preview": "vite preview"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "7.3.4",
    "@mui/material": "7.3.4",
    "axios": "1.12.2",
    "i18next": "25.6.0",
    "i18next-browser-languagedetector": "8.2.0",
    "npm-run-all2": "8.0.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-i18next": "16.2.1",
    "react-router-dom": "6.30.1"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@types/node": "22.18.12",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.1.0",
    "eslint": "9.38.0",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "ncp": "2.0.0",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2",
    "vite": "7.1.12"
  }
}
