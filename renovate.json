{
  "extends": [
    "github>entur/ror-renovate-config",
    "group:allNonMajor",
    "schedule:weekly"
  ],
  "labels": [
    "skip changelog"
  ],
  "packageRules": [
    {
      "matchFileNames": [
        "admin-ui/package.json"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "groupName": "admin-ui non-major dependencies",
      "groupSlug": "admin-ui-non-major",
      "labels": [
        "skip changelog"
      ]
    }
  ]
}
